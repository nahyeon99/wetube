mixin videoBlock(video = {})
    .videoBlock
        a(href=routes.videoDetail(video.id))
            video.videoBlock__thumbnail(src="https://www.learningcontainer.com/wp-content/uploads/2020/05/sample-mp4-file.mp4", controls=true)
            h4.videoBlock__title=video.title 
            h6.videoBlock__views=video.views